{
  "name": "Unstructured Add-on",
  "version": "0.18.21",
  "slug": "unstructured",
  "description": "Run the official Unstructured container (v0.18.21) as a Home Assistant add-on",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "image": "downloads.unstructured.io/unstructured-io/unstructured:latest",
  "ports": {
    "8080/tcp": 8080
  },
  "map": ["config:rw"],
  "options": {
    "api_key": "",
    "port": 8080
  },
  "schema": {
    "api_key": "str?",
    "port": "int"
  }
}
